{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/socket/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/socket/tsconfig.app.json",
    "assets": ["**/*.proto"],
    "watchAssets": true
  },
  "generateOptions": {
    "spec": false
  },
  "monorepo": true,
  "root": "apps/socket",
  "projects": {
    "socket": {
      "type": "application",
      "root": "apps/socket",
      "entryFile": "main",
      "sourceRoot": "apps/socket/src",
      "compilerOptions": {
        "tsConfigPath": "apps/socket/tsconfig.app.json"
      }
    },
    "common": {
      "type": "application",
      "root": "apps/common",
      "entryFile": "main",
      "sourceRoot": "apps/common/src",
      "compilerOptions": {
        "tsConfigPath": "apps/common/tsconfig.app.json"
      }
    },
    "worker": {
      "type": "application",
      "root": "apps/worker",
      "entryFile": "main",
      "sourceRoot": "apps/worker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/worker/tsconfig.app.json"
      }
    },
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "file": {
      "type": "application",
      "root": "apps/file",
      "entryFile": "main",
      "sourceRoot": "apps/file/src",
      "compilerOptions": {
        "tsConfigPath": "apps/file/tsconfig.app.json"
      }
    }
  }
}